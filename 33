
   let ThankYou = setInterval(function() {
                   var GreenAlert = document.querySelector("body > main > div > div > div > div > p")
                    if (GreenAlert) {
            const isVisible = !!(GreenAlert.offsetWidth || GreenAlert.offsetHeight || GreenAlert.getClientRects().length);
            if (isVisible) {
